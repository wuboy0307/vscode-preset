{
  "name": "vscode-preset",
  "displayName": "vscode-preset",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:vscode-preset.applyDefaults",
    "onCommand:vscode-preset.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-preset.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "vscode-preset.applyDefaults",
        "title": "Apply Recommended Workspace Settings"
      }
    ],
    "configurationDefaults": {
      "editor.formatOnSave": true,
      "editor.tabSize": 2,
      "files.trimTrailingWhitespace": true
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.108.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.52.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
